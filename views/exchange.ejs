          </div>
          <div class="col-md-6 col-xs-6">
            <h2>Crypto</h2>
            <h1>
              <span class="label label-info">{{total_exchange + total_ico + total_billy - total_usd | number:2}}</span>
            </h1>
            <h2>{{(total_exchange + total_ico + total_billy - total_usd) / (total_exchange + total_ico + total_billy) * 100 |
              number:2}}%
            </h2>
          </div>
        </div>
        <div class="row">
          <div class="col-md-6">
            <h3 class="label quoine">quoinex</h3>
            <table class="table">
              <thead>
                <tr>
                  <th>Currency</th>
                  <th>Balance</th>
                  <th>USD</th>
                  <th>Price</th>
                  <th>Change 24h</th>
                </tr>
              </thead>
              <tbody>
                <tr ng-repeat="balance in balance_quoinex">
                  <td>{{balance.currency}}</td>
                  <td>{{balance.balance}}</td>
                  <td ng-init="item = balance.balance * ticker[balance.currency].price; $parent.total_quoine = $parent.total_quoine + item">{{item | number:2}}</td>
                  <td>{{ticker[balance.currency].price}}</td>
                  <td ng-class="ticker[balance.currency].percent_change_24h > 0 ? 'success' : 'danger'">{{ticker[balance.currency].percent_change_24h}}%</td>
                </tr>
                <tr class="info">
                  <td colspan="2">Total</td>
                  <td>{{total_quoine | number:2}}</td>
                  <td></td>
                  <td></td>
                </tr>
              </tbody>
            </table>
